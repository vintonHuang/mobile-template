{
  "name": "vue-h5-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --mode development",
    "build:dev": "vue-tsc --noEmit && vite build --mode development",
    "build:sit": "vue-tsc --noEmit && vite build --mode sit",
    "build:pre": "vue-tsc --noEmit && vite build --mode pre",
    "build:pro": "vue-tsc --noEmit && vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "prettier": "prettier --write .",
    "commit": "npm run prettier && git add . && git-cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^8.6.0",
    "axios": "^0.19.2",
    "consola": "^2.15.3",
    "dc-vizier-track": "^1.0.6",
    "eslint-plugin-vue": "^8.5.0",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.0",
    "pinia": "^2.0.0-rc.10",
    "vant": "^3.5.0",
    "vizier-gateway": "^1.0.0",
    "vue": "^3.2.25",
    "vue-i18n": "^9.1.10",
    "vue-router": "4"
  },
  "devDependencies": {
    "@types/mockjs": "^1.0.6",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "cross-env": "^7.0.3",
    "dart-sass": "^1.25.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "git-cz": "^4.8.0",
    "husky": ">=6",
    "less": "^4.1.2",
    "lint-staged": ">=10",
    "mockjs": "^1.1.0",
    "mrm": "^3.0.10",
    "postcss-less": "^6.0.0",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.5.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.8.8",
    "unplugin-vue-components": "^0.19.6",
    "vite": "^2.8.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eruda": "^1.0.1",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-pages": "^0.24.0",
    "vite-plugin-progress": "^0.0.2",
    "vite-plugin-restart": "^0.1.1",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-tsc": "^0.29.8"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx,scss,less,styl,html}": [
      "yarn lint",
      "prettier --write",
      "git add"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx,vue,js,jsx}": "eslint --cache --fix"
  }
}
